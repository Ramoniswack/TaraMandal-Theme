{
  "name": "TaraMandal Pro",
  "type": "dark",
  "colors": {
    // Core editor colors
    "editor.background": "#1A1A2E",
    "editor.foreground": "#E0E0FF",
    "editorCursor.foreground": "#FF6D92",
    "editor.lineHighlightBackground": "#1F1F3A",
    "editorIndentGuide.background": "#2A2A4A",
    "editorIndentGuide.activeBackground": "#FF6D9255",
    
    // Selection and highlighting
    "editor.selectionBackground": "#FF6D9235",
    "editor.selectionHighlightBackground": "#FF6D9220",
    "editor.wordHighlightBackground": "#7BE0FF25",
    "editor.wordHighlightStrongBackground": "#7BE0FF35",
    "editor.findMatchBackground": "#FFAB7B55",
    "editor.findMatchHighlightBackground": "#FFAB7B35",
    "editor.rangeHighlightBackground": "#313159",
    
    // Line numbers with better contrast
    "editorLineNumber.foreground": "#565687",
    "editorLineNumber.activeForeground": "#FF6D92",
    "editorRuler.foreground": "#2A2A4A",
    
    // Bracket matching
    "editorBracketMatch.background": "#FF6D9240",
    "editorBracketMatch.border": "#FF6D92",
    
    // Gutter and margins
    "editorGutter.background": "#16162C",
    "editorGutter.modifiedBackground": "#7BE0FF",
    "editorGutter.addedBackground": "#A4E8C9",
    "editorGutter.deletedBackground": "#FF6D92",
    
    // Activity Bar
    "activityBar.background": "#121224",
    "activityBar.foreground": "#D6C5FD",
    "activityBar.inactiveForeground": "#565687",
    "activityBarBadge.background": "#FF6D92",
    "activityBarBadge.foreground": "#FFFFFF",
    
    // Side Bar
    "sideBar.background": "#16162C",
    "sideBar.foreground": "#D6C5FD",
    "sideBar.border": "#2A2A4A",
    "sideBarTitle.foreground": "#E0E0FF",
    "sideBarSectionHeader.background": "#1A1A2E",
    "sideBarSectionHeader.foreground": "#D6C5FD",
    
    // Title Bar
    "titleBar.activeBackground": "#121224",
    "titleBar.activeForeground": "#E0E0FF",
    "titleBar.inactiveBackground": "#0F0F1F",
    "titleBar.inactiveForeground": "#8A8AB8",
    "titleBar.border": "#2A2A4A",
    
    // Status Bar
    "statusBar.background": "#121224",
    "statusBar.foreground": "#E0E0FF",
    "statusBar.noFolderBackground": "#121224",
    "statusBar.debuggingBackground": "#FF6D92",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBarItem.hoverBackground": "#1F1F3A",
    "statusBarItem.remoteBackground": "#7BE0FF",
    "statusBarItem.remoteForeground": "#121224",
    
    // Tabs
    "editorGroupHeader.tabsBackground": "#16162C",
    "editorGroupHeader.tabsBorder": "#2A2A4A",
    "tab.activeBackground": "#1A1A2E",
    "tab.activeForeground": "#E0E0FF",
    "tab.activeBorder": "#FF6D92",
    "tab.inactiveBackground": "#16162C",
    "tab.inactiveForeground": "#8A8AB8",
    "tab.hoverBackground": "#1F1F3A",
    "tab.border": "#2A2A4A",
    
    // Terminal
    "terminal.background": "#16162C",
    "terminal.foreground": "#E0E0FF",
    "terminalCursor.background": "#FF6D92",
    "terminalCursor.foreground": "#FFFFFF",
    
    // Lists and input controls
    "list.activeSelectionBackground": "#FF6D9235",
    "list.activeSelectionForeground": "#E0E0FF",
    "list.inactiveSelectionBackground": "#2A2A4A",
    "list.hoverBackground": "#1F1F3A",
    "input.background": "#1A1A2E",
    "input.foreground": "#E0E0FF",
    "input.border": "#2A2A4A",
    "button.background": "#FF6D92",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#FF8FAF"
  },
  "tokenColors": [
    {
      "name": "Comments - Muted but readable",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#6B7394",
        "fontStyle": "italic"
      }
    },
    
    // STRINGS - GREEN FAMILY (easy to spot text content)
    {
      "name": "Strings - Bright Green",
      "scope": [
        "string.quoted.single",
        "string.quoted.double",
        "string.quoted.triple"
      ],
      "settings": {
        "foreground": "#4AE54A"
      }
    },
    {
      "name": "Template Strings - Lighter Green",
      "scope": [
        "string.template",
        "string.interpolated",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#66FF66"
      }
    },
    {
      "name": "Regex Strings - Yellow Green",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#9AFF66"
      }
    },
    
    // NUMBERS & CONSTANTS - ORANGE FAMILY
    {
      "name": "Numbers - Bright Orange",
      "scope": [
        "constant.numeric",
        "constant.language.numeric"
      ],
      "settings": {
        "foreground": "#FF8533"
      }
    },
    {
      "name": "Boolean Constants - Light Orange",
      "scope": [
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#FFB366",
        "fontStyle": "bold"
      }
    },
    
    // KEYWORDS - PINK/RED FAMILY (control flow stands out)
    {
      "name": "Control Keywords - Bright Pink",
      "scope": [
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.flow",
        "keyword.control.return",
        "keyword.control.break",
        "keyword.control.continue",
        "keyword.control.switch",
        "keyword.control.case",
        "keyword.control.default"
      ],
      "settings": {
        "foreground": "#FF3366",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Declaration Keywords - Medium Pink",
      "scope": [
        "storage.type",
        "storage.modifier",
        "keyword.other.type",
        "keyword.other.var",
        "keyword.other.let",
        "keyword.other.const"
      ],
      "settings": {
        "foreground": "#FF6699",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Other Keywords - Light Pink",
      "scope": [
        "keyword",
        "keyword.operator.new",
        "keyword.other.await",
        "keyword.other.async"
      ],
      "settings": {
        "foreground": "#FF99CC"
      }
    },
    
    // FUNCTIONS - BLUE FAMILY (functions are core to code)
    {
      "name": "Function Declarations - Bright Blue",
      "scope": [
        "entity.name.function",
        "meta.function-call.generic",
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#3399FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function Calls - Light Blue",
      "scope": [
        "variable.function",
        "meta.function-call",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#66B3FF"
      }
    },
    {
      "name": "Methods - Cyan Blue",
      "scope": [
        "entity.name.method",
        "variable.other.property.method"
      ],
      "settings": {
        "foreground": "#33CCFF",
        "fontStyle": "italic"
      }
    },
    
    // VARIABLES - WHITE/GRAY FAMILY (neutral, readable)
    {
      "name": "Variables - Bright White",
      "scope": [
        "variable.other",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Parameters - Light Gray",
      "scope": [
        "variable.parameter",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#E6E6FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Object Properties - Medium Gray",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#CCCCFF"
      }
    },
    
    // CLASSES & TYPES - PURPLE FAMILY
    {
      "name": "Class Names - Bright Purple",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#9933FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types & Interfaces - Light Purple",
      "scope": [
        "entity.name.type",
        "entity.name.type.interface",
        "support.type"
      ],
      "settings": {
        "foreground": "#B866FF"
      }
    },
    {
      "name": "Generics - Medium Purple",
      "scope": [
        "entity.name.type.parameter",
        "meta.type.parameters"
      ],
      "settings": {
        "foreground": "#CC99FF"
      }
    },
    
    // IMPORTS/EXPORTS - CYAN FAMILY (module system)
    {
      "name": "Import/Export Keywords - Bright Cyan",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as",
        "keyword.control.require"
      ],
      "settings": {
        "foreground": "#00FFCC",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Module Names - Light Cyan",
      "scope": [
        "string.quoted.module",
        "variable.other.module"
      ],
      "settings": {
        "foreground": "#66FFDD"
      }
    },
    
    // OPERATORS - YELLOW FAMILY
    {
      "name": "Operators - Bright Yellow",
      "scope": [
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#FFDD33"
      }
    },
    {
      "name": "Special Operators - Light Yellow",
      "scope": [
        "keyword.operator.spread",
        "keyword.operator.destructuring",
        "keyword.operator.optional"
      ],
      "settings": {
        "foreground": "#FFEE66"
      }
    },
    
    // PUNCTUATION - NEUTRAL
    {
      "name": "Punctuation - Medium Gray",
      "scope": [
        "punctuation",
        "meta.brace",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#B0B0D0"
      }
    },
    
    // MARKUP & WEB LANGUAGES
    {
      "name": "HTML Tags - Bright Red",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml"
      ],
      "settings": {
        "foreground": "#FF4444",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Attributes - Light Blue",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#66B3FF"
      }
    },
    {
      "name": "CSS Properties - Cyan",
      "scope": [
        "support.type.property-name.css",
        "meta.property-name"
      ],
      "settings": {
        "foreground": "#33CCFF"
      }
    },
    {
      "name": "CSS Values - Green",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value"
      ],
      "settings": {
        "foreground": "#66FF66"
      }
    },
    {
      "name": "JSON Keys - Purple",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#B866FF"
      }
    },
    
    // MARKDOWN
    {
      "name": "Markdown Headings - Bright Pink",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#FF3366",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Links - Blue",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#3399FF"
      }
    },
    {
      "name": "Markdown Code - Green",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#4AE54A",
        "background": "#1F1F3A"
      }
    },
    
    // ERROR HIGHLIGHTING
    {
      "name": "Invalid/Error - Red Background",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#FF3366"
      }
    },
    
    // LANGUAGE SPECIFIC IMPROVEMENTS
    {
      "name": "Python Decorators - Magenta",
      "scope": [
        "entity.name.function.decorator.python",
        "meta.function.decorator.python"
      ],
      "settings": {
        "foreground": "#FF66CC"
      }
    },
    {
      "name": "JavaScript Console - Yellow",
      "scope": [
        "support.type.object.console.js"
      ],
      "settings": {
        "foreground": "#FFDD33"
      }
    },
    {
      "name": "React JSX - Light Red",
      "scope": [
        "entity.name.tag.jsx",
        "support.class.component.jsx"
      ],
      "settings": {
        "foreground": "#FF6666"
      }
    }
  ]
}