  "tokenColors": [
    {
      "name": "Comments - Subtle but readable",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#7C7C9C",
        "fontStyle": "italic"
      }
    },
    
    // STRINGS - GREEN FAMILY (natural, easy to read)
    {
      "name": "Strings - Forest Green",
      "scope": [
        "string.quoted.single",
        "string.quoted.double",
        "string.quoted.triple"
      ],
      "settings": {
        "foreground": "#198754"
      }
    },
    {
      "name": "Template Strings - Emerald",
      "scope": [
        "string.template",
        "string.interpolated",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#20C997"
      }
    },
    {
      "name": "Regex Strings - Teal",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#20C997"
      }
    },
    
    // NUMBERS & CONSTANTS - ORANGE FAMILY
    {
      "name": "Numbers - Warm Orange",
      "scope": [
        "constant.numeric",
        "constant.language.numeric"
      ],
      "settings": {
        "foreground": "#FD7E14"
      }
    },
    {
      "name": "Boolean Constants - Light Orange",
      "scope": [
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#FF8511",
        "fontStyle": "bold"
      }
    },
    
    // KEYWORDS - PINK/RED FAMILY (commands stand out)
    {
      "name": "Control Keywords - Deep Rose",
      "scope": [
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.flow",
        "keyword.control.return",
        "keyword.control.break",
        "keyword.control.continue",
        "keyword.control.switch",
        "keyword.control.case",
        "keyword.control.default"
      ],
      "settings": {
        "foreground": "#D63384",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Declaration Keywords - Medium Pink",
      "scope": [
        "storage.type",
        "storage.modifier",
        "keyword.other.type",
        "keyword.other.var",
        "keyword.other.let",
        "keyword.other.const"
      ],
      "settings": {
        "foreground": "#E91E63",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Other Keywords - Soft Pink",
      "scope": [
        "keyword",
        "keyword.operator.new",
        "keyword.other.await",
        "keyword.other.async"
      ],
      "settings": {
        "foreground": "#F06292"
      }
    },
    
    // FUNCTIONS - BLUE FAMILY (professional, trustworthy)
    {
      "name": "Function Declarations - Deep Blue",
      "scope": [
        "entity.name.function",
        "meta.function-call.generic",
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#0D47A1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function Calls - Medium Blue",
      "scope": [
        "variable.function",
        "meta.function-call",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#1976D2"
      }
    },
    {
      "name": "Methods - Light Blue",
      "scope": [
        "entity.name.method",
        "variable.other.property.method"
      ],
      "settings": {
        "foreground": "#2196F3",
        "fontStyle": "italic"
      }
    },
    
    // VARIABLES - DARK GRAY/BLACK (readable, neutral)
    {
      "name": "Variables - Dark Text",
      "scope": [
        "variable.other",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#212529"
      }
    },
    {
      "name": "Parameters - Medium Gray",
      "scope": [
        "variable.parameter",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#495057",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Object Properties - Slate",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#6C757D"
      }
    },
    
    // CLASSES & TYPES - PURPLE FAMILY (elegant, structured)
    {
      "name": "Class Names - Deep Purple",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#6F42C1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types & Interfaces - Medium Purple",
      "scope": [
        "entity.name.type",
        "entity.name.type.interface",
        "support.type"
      ],
      "settings": {
        "foreground": "#8E44AD"
      }
    },
    {
      "name": "Generics - Light Purple",
      "scope": [
        "entity.name.type.parameter",
        "meta.type.parameters"
      ],
      "settings": {
        "foreground": "#A569BD"
      }
    },
    
    // IMPORTS/EXPORTS - TEAL FAMILY (modular, organized)
    {
      "name": "Import/Export Keywords - Dark Teal",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as",
        "keyword.control.require"
      ],
      "settings": {
        "foreground": "#0F766E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Module Names - Medium Teal",
      "scope": [
        "string.quoted.module",
        "variable.other.module"
      ],
      "settings": {
        "foreground": "#14B8A6"
      }
    },
    
    // OPERATORS - AMBER/YELLOW (mathematical, logical)
    {
      "name": "Operators - Golden Yellow",
      "scope": [
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#F59E0B"
      }
    },
    {
      "name": "Special Operators - Amber",
      "scope": [
        "keyword.operator.spread",
        "keyword.operator.destructuring",
        "keyword.operator.optional"
      ],
      "settings": {
        "foreground": "#FBBF24"
      }
    },
    
    // PUNCTUATION - NEUTRAL GRAY
    {
      "name": "Punctuation - Medium Gray",
      "scope": [
        "punctuation",
        "meta.brace",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#6C757D"
      }
    },
    
    // MARKUP & WEB LANGUAGES
    {
      "name": "HTML Tags - Crimson",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml"
      ],
      "settings": {
        "foreground": "#DC2626",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Attributes - Blue",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#1976D2"
      }
    },
    {
      "name": "CSS Properties - Teal",
      "scope": [
        "support.type.property-name.css",
        "meta.property-name"
      ],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "name": "CSS Values - Green",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value"
      ],
      "settings": {
        "foreground": "#198754"
      }
    },
    {
      "name": "JSON Keys - Purple",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#8E44AD"
      }
    },
    
    // MARKDOWN
    {
      "name": "Markdown Headings - Deep Rose",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#D63384",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Links - Blue",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#0D47A1"
      }
    },
    {
      "name": "Markdown Code - Green",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#198754"
      }
    },
    
    // ERROR HIGHLIGHTING
    {
      "name": "Invalid/Error - Red",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#DC2626",
        "fontStyle": "bold underline"
      }
    },
    
    // LANGUAGE SPECIFIC
    {
      "name": "Python Decorators - Magenta",
      "scope": [
        "entity.name.function.decorator.python",
        "meta.function.decorator.python"
      ],
      "settings": {
        "foreground": "#E91E63"
      }
    },
    {
      "name": "JavaScript Console - Orange",
      "scope": [
        "support.type.object.console.js"
      ],
      "settings": {
        "foreground": "#F59E0B"
      }
    },
    {
      "name": "React JSX - Light Red",
      "scope": [
        "entity.name.tag.jsx",
        "support.class.component.jsx"
      ],
      "settings": {
        "foreground": "#EF4444"
      }
    }
  ]
}