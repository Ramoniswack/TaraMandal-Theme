{
  "name": "taramandal",
  "displayName": "TaraMandal",
  "version": "0.0.3",
  "publisher": "ramoniswack",
  "icon": "logo/taramandal-logo.png",
  "description": "A cosmic-inspired theme collection for VS Code with stellar accents",
  "engines": {
    "vscode": "^1.70.0"
  },
  "main": "./extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "TaraMandal",
        "uiTheme": "vs-dark",
        "path": "./themes/taramandal-color-theme.json"
      },
      {
        "label": "TaraMandal Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/taramandal-dark-color-theme.json"
      },
      {
        "label": "TaraMandal Light Cream",
        "uiTheme": "vs",
        "path": "./themes/taramandal-light-color-theme.json"
      },
      {
        "label": "TaraMandal Aurora",
        "uiTheme": "vs-dark",
        "path": "./themes/taramandal-aurora-color-theme.json"
      }
    ],
    "configuration": {
      "title": "TaraMandal Theme",
      "properties": {
        "taramandal.terminalBackground": {
          "type": "string",
          "default": "#FAF5ED",
          "description": "Terminal background color for light theme"
        }
      }
    }
  },
  "keywords": [
    "theme",
    "dark theme",
    "light theme",
    "cosmic",
    "stellar",
    "aurora",
    "space",
    "night",
    "minimal"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ramoniswack/TaraMandal-Theme.git"
  },
  "devDependencies": {
    "vscode": "^1.1.37"
  }
}